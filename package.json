{
  "name": "testdata-kun",
  "displayName": "testdata-kun",
  "description": "Generate dummy data for TypeScript type declarations",
  "version": "1.0.0",
  "private": true,
  "publisher": "josha0ngel03",
  "engines": {
    "vscode": "1.91.0"
  },
  "categories": [
    "AI",
    "Chat"
  ],
  "enabledApiProposals": [
    "chatVariableResolver"
  ],
  "activationEvents": [],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "testdata-kun.generate",
        "title": "Generate dummy data"
      }
    ],
    "chatParticipants": [
      {
        "id": "chat.testdata-kun",
        "name": "testdata-kun",
        "fullName": "Test Data-kun",
        "description": "Dummy data generation",
        "isSticky": true
      }
    ]
  },
  "scripts": {
    "compile": "pnpm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "pnpm run package",
    "package": "pnpm run check-types && node esbuild.js --production",
    "vscode:package": "vsce package --no-dependencies",
    "vscode:publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "10.0.7",
    "@types/node": "~20.14.9",
    "@types/vscode": "1.91.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "@vscode/test-cli": "0.0.9",
    "@vscode/test-electron": "2.4.0",
    "@vscode/vsce": "2.29.0",
    "esbuild": "0.23.0",
    "eslint": "8.57.0",
    "typescript": "5.5.3"
  },
  "dependencies": {
    "ts-morph": "22.0.0"
  }
}
